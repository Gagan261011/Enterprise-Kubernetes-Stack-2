# Example terraform.tfvars file
# Copy this to terraform.tfvars and customize

aws_region           = "ap-south-1"
project_name         = "enterprise-k8s-shop"
environment          = "demo"
vpc_cidr             = "10.0.0.0/16"
instance_type        = "t3.medium"
master_count         = 1
worker_count         = 3
key_name             = "your-aws-key-pair-name"  # CHANGE THIS
ssh_private_key_path = "~/.ssh/your-key.pem"     # CHANGE THIS
allowed_ssh_ip       = "0.0.0.0/0"               # CHANGE to your IP for security
